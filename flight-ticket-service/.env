# Environment Configuration for Flight Ticket Service

# Server Configuration
PORT=8080

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=[Google Cloud Project ID]
GOOGLE_APPLICATION_CREDENTIALS=[Credentials File]

